<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/a100px"
    android:orientation="horizontal"

    >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/a28px"
        android:orientation="horizontal"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@mipmap/logo"

            />
        <TextView

            android:textColor="#333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/a10px"
            android:text="银联快捷"
           android:id="@+id/quick_lv_type"
            />
    </LinearLayout>

    <TextView
        android:textColor="#333333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:text="500-30000"
        android:id="@+id/quick_lv_money"
        />

    <TextView
        android:textColor="#333333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.40%"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/a110px"
        android:layout_centerVertical="true"
        android:id="@+id/quick_lv_huilv"
        />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/weixuanzhong"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/a28px"
        android:layout_centerVertical="true"
        />

</RelativeLayout>
</LinearLayout>
