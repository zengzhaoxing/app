<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:background="@color/white"
        android:id="@+id/annex_lv_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/a100px"

        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/a30px"
            android:text=" "
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/a28px"
            android:textColor="#333333"
            android:id="@+id/annex_lv_name"

            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/zhishijiantou"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/a28px"
            android:id="@+id/enterenter"
            />




    </RelativeLayout>
</LinearLayout>
